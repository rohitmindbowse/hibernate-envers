	<properties>
		<java.version>17</java.version>
		<model.mapper.version>3.2.0</model.mapper.version>
		<springdoc.openapi.version>2.3.0</springdoc.openapi.version>
		<nimbusds.version>9.37.3</nimbusds.version>
